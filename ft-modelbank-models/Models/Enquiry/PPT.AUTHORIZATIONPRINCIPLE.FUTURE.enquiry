Enquiry "PPT.AUTHORIZATIONPRINCIPLE.FUTURE" for "name:/:PPT.AUTHORIZATIONPRINCIPLE#" {
    metamodelVersion: "1.30.6"
    header {
        label {
            en = "Authorization Principle List - Future"
        }

        column: 15
        line: 0
    }

    description {
        en = "Authorization Principle Search"
    }

    zero-records-display: true
    no-selection: true
    start-line: 4
    end-line: 99 fixed-selection "RSCAuthPrinciple" {
        Equals "\'L\'"
    }

    fixed-selection "RACAuthPrinciple" {
        Equals "\'F\'"
    }

    fixed-sort: "ProcessingCompany" Null
    fixed-sort: "Ranking" Null
    custom-selection {

        field "@ID" {
            label {
                en = "AuthPrinciple ID"
            }

        }

        AND

        field "ProcessingCompany" {
            label {
                en = "Company"
            }

        }

        AND

        field "Ranking" {
            label {
                en = "Rank"
            }

        }

        AND

        field "Status" {
            label {
                en = "Status"
            }

        }

        AND

        field "Direction" {
            label {
                en = "Direction"
            }

        }

        AND

        field "TransactionAmountLowerLimit" {
            label {
                en = "Tran Amt Lower Lmt"
            }

        }

        AND

        field "TransactionAmountUpperLimit" {
            label {
                en = "Tran Amt Upper Lmt"
            }

        }

        AND

        field "TransactionCurrencyCode" {
            label {
                en = "Txn Ccy"
            }

        }

        AND

        field "LowerMessagePriority" {
            label {
                en = "Lower Msg Pty"
            }

        }

        AND

        field "UpperMessagePriority" {
            label {
                en = "Upper Msg Pty"
            }

        }

        AND

        field "IncomingMessageType" {
            label {
                en = "Msg Type"
            }

        }

        AND

        field "OriginatingSource" {
            label {
                en = "Source"
            }

        }

        AND

        field "CTRBTRIndicator" {
            label {
                en = "CTR/BTR"
            }

        }

        AND

        field "DebitBusinessLine" {
            label {
                en = "Dbt Business Line"
            }

        }

        AND

        field "CreditBusinessLine" {
            label {
                en = "Cdt Business Line"
            }

        }

        AND

        field "HighWeightCode" {
            label {
                en = "High Weight Code"
            }

        }

        AND

        field "AuthorizationPrinciple" {
            label {
                en = "Auth Principle"
            }

        }

        None
    }

    field "@ID" {
        label {
            en = "AuthPrinciple ID"
        }

        length: 255
        alignment: Left
        comma-separator: false
        position {
            column: 1
        }

        processing-mode: Single
        hidden: true
        operation: application-field-name "@ID"
    }

    field "ProcessingCompany" {
        label {
            en = "Company"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 2
        }

        processing-mode: Single
        operation: application-field-name "ProcessingCompany"
    }

    field "Ranking" {
        label {
            en = "Rank"
        }

        length: 9
        alignment: Right
        comma-separator: false
        position {
            column: 3
        }

        processing-mode: Single
        operation: application-field-name "Ranking"
    }

    field "Status" {
        label {
            en = "Status"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 4
        }

        processing-mode: Single
        operation: application-field-name "Status"
    }

    field "Direction" {
        label {
            en = "Direction"
        }

        length: 1
        alignment: Left
        comma-separator: false
        position {
            column: 5
        }

        processing-mode: Single
        operation: application-field-name "Direction"
    }

    field "ID" {
        length: 255
        alignment: Left
        comma-separator: false
        processing-mode: Single
