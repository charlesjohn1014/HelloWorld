Enquiry "KYC.UXP" for "name:/CR_Analytical:CR.CONTACT.LOG#" {
    metamodelVersion: "1.30.6"
    description {
        en = "Reassign Contact Log"
    }

    zero-records-display: true
    no-selection: true
    start-line: 4
    end-line: 99 fixed-selection "CONTACT.TYPE" {
        NotEquals "TRANSACTION"
    }

    custom-selection {

        field "CONTACT.CLIENT" {
            label {
                en = "Contact Client"
            }

        }

        AND

        field "CONTACT.STAFF" {
            label {
                en = "Contact Staff"
            }

        }

        AND

        field "CONTACT.DATE" {
            label {
                en = "Contact Date"
            }

        }

        None
    }

    field "TITLE" {
        display-type: "CLASS-ENQ.H.TITLE" break-condition {
            break: Once
        }

        comma-separator: false
        display-section: Header
        position {
            column: 10
            line: 1
        }

        operation: constant "\"Reassign Contact Log\""
    }

    field "ID" {
        label {
            en = "Id"
        }

        length: 35
        alignment: Left
        comma-separator: false
        position {
            column: 1
        }

