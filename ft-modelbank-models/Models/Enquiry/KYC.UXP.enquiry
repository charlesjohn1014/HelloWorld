        processing-mode: Single
        operation: application-field-name "CONTACT.TYPE"
    }

    field "CONTACT.STATUS" {
        label {
            en = "Contact Status"
        }

        length: 20
        alignment: Left
        comma-separator: false
        position {
            column: 35
        }

        processing-mode: Single
        operation: application-field-name "CONTACT.STATUS"
    }

    field "CONTACT.DESC" {
        label {
            en = "Description"
        }

        length: 20
        alignment: Left
        comma-separator: false
        position {
            column: 57
        }

        processing-mode: Single
