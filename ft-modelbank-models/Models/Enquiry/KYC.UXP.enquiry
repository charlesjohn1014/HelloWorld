        position {
            column: 104
        }

        processing-mode: Multi
        operation: application-field-name "CONTACT.STAFF"
    }

    field "CONTACT.DATE" {
        label {
            en = "Contact Date"
        }

        format: Date
        length: 11
        alignment: Left
        comma-separator: false
        position {
            column: 146
        }

        processing-mode: Single
        operation: application-field-name "CONTACT.DATE"
    }

    field "BLANK" {
        comma-separator: false
        operation: constant "\"\""
    }

    field "CONTACT.TIME" {
        label {
            en = "Contact Time"
