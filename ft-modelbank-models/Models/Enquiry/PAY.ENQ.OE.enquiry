    metamodelVersion: "1.30.6"
    header {
        label {
            en = "Pending Order Entry Payments"
        }

        column: 15
        line: 0
    }

    full-description {
        en = "Pending Order Entry Payments"
    }

    description {
        en = "Pending Order Entry Payments"
    }

    zero-records-display: true
    no-selection: true
    start-line: 4
    end-line: 19 fixed-selection "StatusCode" {
