        None
    }

    field "Company" {
        label {
            en = "Company"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 1
        }

        processing-mode: Single
        operation: application-field-name "CompanyID"
    }

    field "FTnumber" {
        label {
            en = "FT Number"
        }

        length: 16
        alignment: Left
        comma-separator: false
        position {
            column: 2
        }

        processing-mode: Single
        operation: application-field-name "FTNumber"
    }

    field "TXN.CCY" {
        label {
            en = "Ccy"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 3
        }

        processing-mode: Single
        operation: application-field-name "TransactionCurrencyCode"
    }

    field "InstructedCurrencyCode" {
        label {
            en = "Instructed Currency"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 4
        }

        processing-mode: Single
        operation: application-field-name "InstructedCurrencyCode"
    }

    field "TransactionAmount" {
        label {
            en = "Amt"
        }

        format: Currency "TXN.CCY" negative-pattern -#
        length: 17
        alignment: Right
        comma-separator: true
        position {
            column: 5
        }

        processing-mode: Single
        operation: application-field-name "TransactionAmount"
    }

    field "InstructedAmount" {
        label {
            en = "Instructed Amount"
        }

        length: 17
        alignment: Right
        comma-separator: false
        position {
            column: 6
        }

        processing-mode: Single
        operation: application-field-name "InstructedAmount"
    }

    field "Direction" {
        label {
            en = "Direction"
        }

        length: 1
        alignment: Left
        comma-separator: false
        position {
            column: 7
        }

        processing-mode: Single
        hidden: true
        operation: application-field-name "PaymentDirection"
    }

    field "DEBIT.ACC" {
        length: 35
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "DebitMainAccount"
    }

    field "CREDIT.ACC" {
        length: 35
        alignment: Right
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "CreditMainAccount"
    }

    field "CREDIT.COMPANY" {
        length: 3
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "CreditMainAccountCompanyID"
    }

    field "DEBIT.COMPANY" {
        length: 3
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "DebitMainAccountCompanyID"
    }

    field "ACCT.COMPANY" {
        label {
            en = "Acc Comp"
        }

        comma-separator: false
        position {
            column: 8
        }

        operation: if "Direction" Equals "I" process "CREDIT.COMPANY" "DEBIT.COMPANY"
    }

    field "DR.CCY" {
        length: 3
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "DebitMainAccountCurrencyCode"
    }

    field "CR.CCY" {
        length: 3
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "CreditMainAccountCurrencyCode"
    }

    field "ACCT.SELECT" {
        label {
            en = "Acc Num"
        }

        comma-separator: false
        position {
            column: 9
        }

        processing-mode: Single
        operation: if "Direction" Equals "I" process "CREDIT.ACC" "DEBIT.ACC"
    }

    field "CCY.SELECT" {
        label {
            en = "Acc Ccy"
        }

        comma-separator: false
        position {
            column: 10
        }

        processing-mode: Single
        operation: if "Direction" Equals "I" process "CR.CCY" "DR.CCY"
    }

    field "EntryDateTime" {
        label {
            en = "Entry Date Time"
        }

        length: 17
        alignment: Left
        comma-separator: false
        position {
            column: 11
        }

        processing-mode: Single
        operation: application-field-name "EntryDateTime"
        conversion: call
        jBC: "@ PP.CONV.LOCALTIMESTAMP"
    }

    field "CSR" {
        length: 16
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "CustomerSpecifiedReference"
    }

    field "TRI" {
        length: 16
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "TransactionReferenceIncoming"
    }

    field "Customer Reference" {
        label {
            en = "Cust Ref"
        }

        comma-separator: false
        position {
            column: 12
        }

        processing-mode: Single
        operation: if "CSR" NotEquals "\"\"" process "CSR" "TRI"
    }

    field "SendersReferenceIncoming" {
        label {
            en = "Sender Ref"
        }

        length: 16
        alignment: Left
        comma-separator: false
        position {
            column: 13
        }

        processing-mode: Single
        operation: application-field-name "SendersReferenceIncoming"
    }

    field "StatusCode" {
        label {
            en = "Status Code"
        }

        length: 3
        alignment: Right
        comma-separator: false
        position {
            column: 14
        }

        processing-mode: Single
        hidden: true
        operation: application-field-name "StatusCode"
    }

    field "StatusDescription" {
        label {
            en = "Description"
        }

        comma-separator: false
        position {
            column: 15
        }

        hidden: true
        operation: field-extract "StatusCode"
        conversion: getFrom "PP.STATUS.CODE" "StatusDescription" false
    }

    field "ProcessingDate" {
        label {
            en = "Date"
        }

        length: 11
        alignment: Right
        comma-separator: false
        position {
            column: 16
        }

        processing-mode: Single
        hidden: true
        operation: application-field-name "ProcessingDate"
    }

    field "BulkSendersReference" {
        length: 50
        alignment: Left
        comma-separator: false
        position {
            column: 17
        }

        processing-mode: Single
        operation: application-field-name "BulkSendersReference"
    }

    field "ID" {
        length: 255
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "ID"
    }

    field "TXN.DRILLDOWN" {
        length: 1
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "PaymentDirection"
        conversion: replace {
            oldData: "B"
            newData: "BOOK.CREATE"
        }

        conversion: replace {
            oldData: "I"
            newData: "IN.CREATE"
        }

        conversion: replace {
            oldData: "O"
            newData: "OUT.CREATE"
        }

        conversion: replace {
            oldData: "R"
            newData: "REDIRECT.CREATE"
        }

    }

    field "VER1" {
        comma-separator: false
        operation: constant "\"PP.ORDER.ENTRY,BTR.\""
    }

    field "VER2" {
        comma-separator: false
        processing-mode: Single
        operation: constant "\"PP.ORDER.ENTRY,CTR.\""
    }

    field "CTRBTR" {
        length: 1
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "CTRBTRIndicator"
    }

    field "FIN.VER" {
        comma-separator: false
        operation: if "CTRBTR" Equals "C" process "VER2" "VER1"
    }

    field "TXN.DRILL" {
        comma-separator: false
        operation: calc "FIN.VER":"TXN.DRILLDOWN"
    }

    field "ZERONORECORDS" {
        display-type: "CLASS-ENQ.NORECS" break-condition {
            break: None
        }

        comma-separator: false
        position {
            column: 1
        }

        operation: constant "\" No records found to display\""
    }

    field "VIEW" {
        comma-separator: false
        operation: constant "\"VIEW\""
    }

    field "SCT.VER" {
        comma-separator: false
        operation: constant "\"PP.ORDER.ENTRY,CTR.SEPACT.OUT.CREATE\""
    }

    field "EXTFIELDS" {
        length: 1
        alignment: Left
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "ExtendedFields"
    }

    field "VIEW.VER1" {
        comma-separator: false
        operation: constant "\"PP.AUTH.ENTRY,VIEW\""
    }

    field "VIEW.VER2" {
        comma-separator: false
        operation: constant "\"PP.AUTH.ENTRY,SEPA.VIEW\""
    }

    field "FIN.VIEW" {
        comma-separator: false
        operation: if "EXTFIELDS" Equals "Y" process "VIEW.VER2" "VIEW.VER1"
    }

    field "FIN.TXN.DRILL" {
        comma-separator: false
        operation: if "EXTFIELDS" Equals "Y" process "SCT.VER" "TXN.DRILL"
    }

    drilldown "1" {
        description {
            en = "Modify"
        }

        label-field: "FTnumber"
        image: "edit.gif"
        criteria: "CURRENT.CompanyID" > "Company"
        criteria: "CURRENT.FTNumber" > "FTnumber" parameters {
            function:  Input
            auto-F3: true
        }

        from-field: "FIN.TXN.DRILL"
    }

    drilldown "2" {
        description {
            en = "View"
        }

        label-field: "FTnumber"
        image: "view.gif"
        criteria: "CURRENT.CompanyID" > "Company"
        criteria: "CURRENT.FTNumber" > "FTnumber"
        criteria: "CURRENT.View" > "VIEW" parameters {
            function:  See
            parameter: "111"
        }

        from-field: "FIN.VIEW"
    }

    drilldown "3" {
        description {
            en = "ViewinDetail"
        }

        label-field: "FTnumber"
        image: "drill.gif" parameters {
            field: "Company"
            field: "FTnumber"
        }

        composite-screen: "PP.PAYMENT.REPAIR.MAIN"
    }

    security {
        application: "PP.COMPANY"
        field: "Company"
    }

}