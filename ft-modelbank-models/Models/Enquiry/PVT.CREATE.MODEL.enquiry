Enquiry "PVT.CREATE.MODEL" for "name:/EB_ModelBank:EB.MODEL.ENQ.LIST#" {
    metamodelVersion: "1.30.6"
    show-all-books: false
    start-line: 4
    end-line: 99 fixed-selection "@ID" {
        NotEquals "\"\""
    }

    field "Field1" {
        comma-separator: false
        operation: constant "\"Create Model\""
    }

    field "Field2" {
        comma-separator: false
        operation: constant "\"Maintain Model\""
    }

    security {
        abort: false
    }

}