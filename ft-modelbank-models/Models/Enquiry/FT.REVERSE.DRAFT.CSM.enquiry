Enquiry "FT.REVERSE.DRAFT.CSM" for "name:/FT_Contract:FUNDS.TRANSFER#" {
    metamodelVersion: "1.30.6" full-description {
        en = "Reverse Drafts"
    }

    description {
        en = "Reverse Drafts"
    }

    zero-records-display: true
    no-selection: true
    start-line: 4
    end-line: 5 fixed-selection "TRANSACTION.TYPE" {
        Equals "\'OD\'"
    }

    custom-selection {

        field "@ID" {
            label {
                en = "Transaction Ref"
            }

        }

        AND

        field "DEBIT.ACCT.NO" {
            label {
                en = "Debit Account No"
            }

        }

        AND

        field "DEBIT.CURRENCY" {
            label {
                en = "Debit Currency"
            }

        }

        AND

        field "DEBIT.AMOUNT" {
            label {
                en = "Debit Amount"
            }

        }

        None
    }

    field "@ID" {
        label {
            en = "Transaction Ref"
        }

        length: 16
        alignment: Left
        comma-separator: false
        position {
            column: 1
        }

        processing-mode: Single
        operation: application-field-name "@ID"
    }

    field "TRANSACTION.TYPE" {
        label {
            en = "Txn Type"
        }

        length: 4
        alignment: Left
        comma-separator: false
        position {
            column: 2
        }

        processing-mode: Single
        operation: application-field-name "TRANSACTION.TYPE"
    }

    field "HEADER1" {
        display-type: "CLASS-TITLE"
        comma-separator: false
        display-section: Header
        position {
            column: 1
            line: 0
        }

        operation: constant "\"Reverse Drafts\""
    }

    field "DEBIT.ACCT.NO" {
        label {
            en = "Debit Account"
        }

        length: 16
        alignment: Right
        comma-separator: false
        position {
            column: 3
        }

        processing-mode: Single
        operation: application-field-name "DEBIT.ACCT.NO"
    }

    field "CCY1" {
        label {
            en = "Debit Ccy"
        }

        length: 3
        alignment: Left
        comma-separator: false
        position {
            column: 4
        }

        processing-mode: Single
        operation: application-field-name "DEBIT.CURRENCY"
    }

    field "AMOUNT.DEBITED" {
        label {
            en = "Debit Amount"
        }

        length: 22
        alignment: Right
        comma-separator: true
        position {
            column: 5
        }

        processing-mode: Single
        operation: application-field-name "AMOUNT.DEBITED"
        conversion: extract from 4 to 19
    }

    field "CREDIT.ACCT.NO" {
        label {
            en = "Credit Account"
        }

        length: 16
        alignment: Right
        comma-separator: false
        position {
            column: 6
        }

        processing-mode: Single
        operation: application-field-name "CREDIT.ACCT.NO"
    }

    field "CCY2" {
        label {
            en = "Credit Ccy"
        }

        comma-separator: false
        position {
            column: 7
        }

        processing-mode: Single
        operation: application-field-name "CREDIT.CURRENCY"
    }

    field "AMOUNT.CREDITED" {
        label {
            en = "Credit Amount"
        }

        length: 22
        alignment: Right
        comma-separator: true
        position {
            column: 8
        }

        processing-mode: Single
        operation: application-field-name "AMOUNT.CREDITED"
        conversion: extract from 4 to 19
    }

    field "INWARD.PAY.TYPE" {
        comma-separator: false
        processing-mode: Single
        operation: application-field-name "INWARD.PAY.TYPE"
    }

    field "NULL" {
        comma-separator: false
        operation: constant "\"\""
    }

    field "TXN.DRILLDOWN" {
        length: 50
        alignment: Left
        comma-separator: false
        operation: field-extract "TRANSACTION.TYPE"
        conversion: replace {
            oldData: "OD"
            newData: "OD.FCY"
        }

        conversion: replace {
            oldData: "OD.10"
            newData: "OD.110"
        }

        conversion: replace {
            oldData: "OT03"
            newData: "OT103.SERIAL"
        }

        conversion: replace {
            oldData: "OT12"
            newData: "OT103.COVER"
        }

        conversion: replace {
            oldData: "OT40"
            newData: "OT400.202"
        }

        conversion: replace {
            oldData: "IT"
            newData: "INWARD.REMIT.HP"
        }

        conversion: replace {
            oldData: "OT20"
            newData: "ACTR.NOST"
        }

        conversion: replace {
            oldData: "OT22"
            newData: "ACTTR.NOST"
        }

    }

    field "APPL" {
        comma-separator: false
        operation: constant "\"FUNDS.TRANSFER,\""
    }

    field "TXN.DRILL" {
        comma-separator: false
        operation: calc "APPL":"TXN.DRILLDOWN"
    }

    field "VER.DEC1" {
        comma-separator: false
        operation: constant "\"FUNDS.TRANSFER,INW\""
    }

    field "VER.DEC2" {
        comma-separator: false
        operation: if "TELEX.FROM.CUST" NotEquals "NULL" process "VER.DEC1" "TXN.DRILL"
    }

    field "VER.DEFAULT" {
        comma-separator: false
        operation: constant "\"FUNDS.TRANSFER,AUTH\""
    }

    field "CHK.VER" {
        comments: "Check and See if the version Exists or not\""
        comma-separator: false
        operation: field-extract "VER.DEC2"
        conversion: getFrom "VERSION" "FIELDS.PER.LINE" false
    }

    field "VER.FINAL" {
        comma-separator: false
        processing-mode: Single
        operation: if "CHK.VER" NotEquals "NULL" process "VER.DEC2" "VER.DEFAULT"
    }

    field "ZERONORECORDS" {
        display-type: "CLASS-ENQ.NORECS" break-condition {
            break: None
        }

        comma-separator: false
        display-section: Header
        position {
            column: 1
        }

        operation: constant "\"No Reverse Drafts\""
    }

    drilldown "1" {
        description {
            en = "View"
        }

        label-field: "@ID"
        image: "view.gif" parameters {
            function:  See
            field: "@ID"
        }

        from-field: "VER.FINAL"
    }

    drilldown "2" {
        description {
            en = "Reverse "
        }

        label-field: "@ID"
        image: "txnreverse.gif" parameters {
            function:  Reverse
            field: "@ID"
        }

        from-field: "VER.FINAL"
    }

}