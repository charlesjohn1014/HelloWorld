Enquiry "EB.TICK.APPL" for "name:/EB_SystemTables:PGM.FILE#" {
    metamodelVersion: "1.30.6"
    start-line: 4
    end-line: 99 fixed-selection "@ID" {
        Equals "LETTER.OF.CREDIT", "MD.DEAL"
    }

    field "@ID" {
        label {
            en = "Application"
        }

        length: 40
        alignment: Left
        comma-separator: false
        position {
            column: 1
        }

        processing-mode: Single
        operation: application-field-name "@ID"
    }

}