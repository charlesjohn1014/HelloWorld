Enquiry "AM.REBAL" for "name:/EB_ModelBank:EB.MODEL.ENQ.LIST#" {
    metamodelVersion: "1.30.6"
    no-selection: true
    start-line: 2
    end-line: 99

    field "USER.PROMPT" {
        display-type: "CLASS-ENQ.H.DATA.LABEL"
        comma-separator: false
        position {
            column: 1
        }

        column-width: 150
        operation: constant "\"Compare & Rebalance\""
    }

    attributes: "NO.TOOLBAR" ; "NO.BREADCRUMBS" ; "NO.COLUMN.HEADER"
}