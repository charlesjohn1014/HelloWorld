Enquiry "AM.VAL.MULTIPIE" for "name:/AM_ModelBank:NOFILE.AM.SVG.SECTOR#" {
    metamodelVersion: "1.30.6"
    description {
        en = "Multi Level Pie View"
    }

    start-line: 4
    end-line: 19 fixed-selection "DATA.ITEM" {
    }

    field "@ID" {
        length: 20
        alignment: Right
        comma-separator: false
        operation: application-field-name "@ID"
    }

    field "AMOUNT" {
        label {
            en = "Amount"
        }

        display-type: "MP-AMOUNT"
        length: 20
        alignment: Right
        comma-separator: false
        position {
            column: 1
        }

        operation: field-extract "@ID"
        conversion: extract from 2 to 0 delimited by "*"
    }

    field "LABEL" {
        label {
            en = "Label"
        }

        display-type: "MP-LABEL"
        format: User
        length: 20
        alignment: Right
        comma-separator: false
        position {
            column: 2
        }

        operation: field-extract "@ID"
        conversion: extract from 3 to 0 delimited by "*"
    }

    field "LEVEL" {
        label {
            en = "Level"
        }

        display-type: "MP-LEVEL"
        length: 20
        alignment: Right
        comma-separator: false
        position {
            column: 3
        }

        operation: field-extract "@ID"
        conversion: extract from 5 to 0 delimited by "*"
    }

    attributes: "ALLDATA"
}