Screen DRAWINGS, IN_756
    t24Name: "DRAWINGS,IN.756"
    metamodelVersion: "1.30.6"
    numberOfAuthorisers: 1
    description: en = "Drawing"
reportLocks: Yes

TransactionFlow {
    otherCompanyAccess: Yes
}

Presentation {
    recordsPerPage: "1"
    fieldsPerLine: "MULTI"
    language: en
    header1: en = "Advice Of Reimbursement Or Payment"
}

Relationship {
    associatedVersions: DRAWINGS,CHGMGN ; DRAWINGS,DEL ; DRAWINGS,COND_DISCREPANCIES ; DRAWINGS,AUDIT
}

API {
    preProcessValidationRoutines:
        jBC: "@V.MB.DRAW.TYPE"
}

attributes: "NO.HEADER.TAB"

Fields {
    "DRAWING_TYPE" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 0
            selectionEnquiry: "AC.ENRICH"
        }

        Translations:
            label: en = "Drawing Type"
            toolTip: en = "Drawing Type"
    }

    "PRESENTOR_CUST" {

        Presentation {
            maxLength: 27
            enrichmentLength: 28
            column: 1
            row: 1
        }

        Translations:
            label: en = "Presentor Id"
    }

    "PRESENTOR" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 2
        }

        Translations:
            label: en = "Presentor Name"
            toolTip: en = "Name/Add of Presentor of the Docs(non-customer)"
        MV: 1
    }

    "PRESENTOR_REF" {

        Presentation {
            maxLength: 27
            enrichmentLength: 20
            column: 1
            row: 3
            hotField: Yes
        }

        Translations:
            label: en = "Presenting Bank\'s Reference"
            toolTip: en = "Presentor\'s reference to include in message/advice"
    }

    "DRAW_CURRENCY" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 4
        }

        Translations:
            label: en = "Claimed Currency"
    }

    "DOCUMENT_AMOUNT" {

        Presentation {
            maxLength: 27
            column: 70
            row: 4
        }

        Translations:
            label: en = "Amount Claimed"
    }

    "VALUE_DATE" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 5
        }

        Translations:
            label: en = "Value Date"
    }

    "MATURITY_REVIEW" {

        Presentation {
            maxLength: 27
            column: 1
            row: 6
        }

        Translations:
            label: en = "Maturity Date"
            toolTip: en = "Maturity date of the Drawings"
    }

    "DRAWDOWN_ACCOUNT" {

        Presentation {
            maxLength: 27
            enrichmentLength: 21
            column: 1
            row: 7
        }

        Translations:
            label: en = "Debit Account"
            toolTip: en = "Account to be debited for this drawing"
    }

    "PAYMENT_METHOD" {

        Presentation {
            maxLength: 27
            column: 1
            row: 8
            selectionEnquiry: "PAY.METHOD.ENQ"
        }

        Translations:
            label: en = "Payment Method"
            toolTip: en = "Method of making payment to the Beneficiary"
    }

    "PAYMENT_ACCOUNT" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 9
            selectionEnquiry: "NOST.OTREMIT"
            enquiryParameter: "CURRENCY EQ DRAW.CURRENCY"
        }

        Translations:
            label: en = "Credit Account"
            toolTip: en = "Account to be credited"
    }

    "SENDER_CORR" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 10
        }

        Translations:
            label: en = "Sender\'s Correspondent"
        MV: 1
    }

    "RECEIVERS_CORR" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 11
        }

        Translations:
            label: en = "Receiver\'s Correspondent"
        MV: 1
    }

    "756_BK_TO_BK" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 12
        }

        Translations:
            label: en = "Sender to Receiver Info"
        MV: 1
    }

    "MT756_REIMB_DATE" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 13
        }

        Translations:
            label: en = "Reimbursed Date"
    }

    "MT756_REIMB_AMT" {

        Presentation {
            maxLength: 27
            enrichmentLength: 35
            column: 1
            row: 14
        }

        Translations:
            label: en = "Reimbursed Amount"
    }

}