        Presentation {
            maxLength: 25
            enrichmentLength: 25
            column: 1
            row: 0
        }

        Translations:
            label: en = "Description"
            toolTip: en = "Enter Description"
    }

    "SWEEP_TYPE" {

        Presentation {
            maxLength: 25
            enrichmentLength: 25
            column: 1
            row: 1
            hotField: Yes
        }

        default: IF ( ) "NULL" THEN "TWOWAY"
        Translations:
            label: en = "Sweep Type"
            toolTip: en = "Specifies sweep type"
    }

    "FREQUENCY" {

        Presentation {
            maxLength: 25
            enrichmentLength: 25
            column: 1
            row: 2
