Screen AC_ACCOUNT_LINK, TWOWAY_RNAU_AUTH
    t24Name: "AC.ACCOUNT.LINK,TWOWAY.RNAU.AUTH"
    metamodelVersion: "1.30.6"
    numberOfAuthorisers: 1
    description: en = "Account Link Table"
reportLocks: Yes

TransactionFlow {
    otherCompanyAccess: Yes
}

Presentation {
    recordsPerPage: "1"
    fieldsPerLine: "MULTI"
    language: en
    header1: en = "Authorise Reverse Two-way Sweep "
}

Relationship {
    nextVersion: AC_CHARGE_REQUEST,INPUT
    nextVersionFunction: Input
    nextVersionTransactionReference: AUTO
