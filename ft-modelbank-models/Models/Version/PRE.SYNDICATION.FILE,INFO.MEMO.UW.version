        Presentation {
            maxLength: 25
            enrichmentLength: 25
            column: 1
            row: 0
        }

        Translations:
            label: en = "Customer"
            toolTip: en = "To enter Customer\'s ID No. or Mnemonic Code"
    }

    "SL_SHORT_DESC" {

        Presentation {
            maxLength: 25
            enrichmentLength: 25
            column: 1
            row: 1
