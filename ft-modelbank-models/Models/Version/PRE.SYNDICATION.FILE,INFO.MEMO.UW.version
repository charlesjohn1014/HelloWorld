Screen PRE_SYNDICATION_FILE, INFO_MEMO_UW
    t24Name: "PRE.SYNDICATION.FILE,INFO.MEMO.UW"
    metamodelVersion: "1.30.6"
    numberOfAuthorisers: 1
    description: en = "SL-Information Memorandum UW"
reportLocks: Yes

TransactionFlow {
    otherCompanyAccess: Yes
}

Presentation {
    recordsPerPage: "1"
    fieldsPerLine: "MULTI"
    language: en
    header1: en = "Syndicate Lending - Circulation of Information Memorandum to Underwriters "
}

Relationship {
    associatedVersions: PRE_SYNDICATION_FILE,INFO_MEMO ; PRE_SYNDICATION_FILE,ROLES ; PRE_SYNDICATION_FILE,AUDIT
}

attributes: "NO.HEADER.TAB"

