        DATE_TIME -> byValue OA_Framework:OA_APPLICATION_DEFINITION__DATE_TIME *
            @t24:i (t24Name=DATE.TIME)
        DEFINITION_VERSION : T24BusinessTypes:ANY
            @t24:i (align=L, inputBehaviour=N, maxLength=5, sysNumber=33.0, core=true, t24Name=DEFINITION.VERSION, mandatory=false)
        DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
            @i18n:Label (en="Dept Code")
            @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=52.0, core=true, t24Name=DEPT.CODE, mandatory=false)
